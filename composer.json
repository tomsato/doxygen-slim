{
    "name": "doxygen-slim",
    "description": "doxygen-slim",
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "3.11.0",
        "slim/php-view": "2.2.0",
        "monolog/monolog": "1.23.0",
        "slim/twig-view": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "Myapp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
